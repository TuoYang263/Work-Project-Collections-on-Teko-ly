{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "TfStateBucketList",
			"Effect": "Allow",
			"Action": [
				"s3:ListBucket",
				"s3:GetBucketLocation"
			],
			"Resource": "arn:aws:s3:::tf-state-438336772967-eu-north-1",
			"Condition": {
				"StringLike": {
					"s3:prefix": [
						"aws-demo/*"
					]
				}
			}
		},
		{
			"Sid": "TfStateObjectsRW",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:DeleteObject"
			],
			"Resource": "arn:aws:s3:::tf-state-438336772967-eu-north-1/aws-demo/*"
		},
		{
			"Sid": "TfLockTable",
			"Effect": "Allow",
			"Action": [
				"dynamodb:GetItem",
				"dynamodb:PutItem",
				"dynamodb:DeleteItem",
				"dynamodb:DescribeTable"
			],
			"Resource": "arn:aws:dynamodb:eu-north-1:438336772967:table/tf-lock-eu-north-1"
		},
		{
			"Sid": "ManageDemoBucketConfig",
			"Effect": "Allow",
			"Action": [
				"s3:GetBucketLocation",
				"s3:GetBucketAcl",
				"s3:GetBucketWebsite",
				"s3:PutBucketWebsite",
				"s3:GetBucketVersioning",
				"s3:PutBucketVersioning",
				"s3:GetBucketPolicy",
				"s3:PutBucketPolicy",
				"s3:GetBucketRequestPayment",
				"s3:PutBucketRequestPayment",
				"s3:GetEncryptionConfiguration",
				"s3:PutEncryptionConfiguration",
				"s3:GetBucketPublicAccessBlock",
				"s3:PutBucketPublicAccessBlock",
				"s3:GetLifecycleConfiguration",
				"s3:PutLifecycleConfiguration",
				"s3:GetBucketCORS",
				"s3:PutBucketCORS",
				"s3:GetBucketTagging",
				"s3:PutBucketTagging",
				"s3:GetBucketLogging",
				"s3:PutBucketLogging",
				"s3:GetReplicationConfiguration",
				"s3:PutReplicationConfiguration",
				"s3:GetBucketObjectLockConfiguration",
				"s3:PutBucketObjectLockConfiguration",
				"s3:ListBucket"
			],
			"Resource": "arn:aws:s3:::my-s3-bucket-for-terraform-tuoyang-*"
		},
		{
			"Sid": "DemoBucketObjects",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:DeleteObject"
			],
			"Resource": "arn:aws:s3:::my-s3-bucket-for-terraform-tuoyang-*/*"
		},
		{
			"Sid": "ManageDemoRole",
			"Effect": "Allow",
			"Action": [
				"iam:CreateRole",
				"iam:DeleteRole",
				"iam:ListRolePolicies",
				"iam:GetRole",
				"iam:UpdateAssumeRolePolicy",
				"iam:AttachRolePolicy",
				"iam:DetachRolePolicy",
				"iam:ListAttachedRolePolicies",
				"iam:TagRole",
				"iam:UntagRole"
			],
			"Resource": "arn:aws:iam::438336772967:role/terraform-demo-role"
		}
	]
}